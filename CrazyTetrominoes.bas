270 DY=CY+1
290 GOSUB 2110
300 GOSUB 710:IF CO=0 THEN CY=DY:GOSUB 2010:GOTO 200
310 GOSUB 410
320 GOTO 150

399 REM
400 REM ==== WE HIT THE GROUND ====
401 REM
410 GOSUB 2010
420 VPOKE 1,$F9C2,0
430 FL=0:IF HL>DY THEN HL=DY
440 FOR Y=HL-1 TO 20
450 X=1:M=(5+Y)*256+(12+X)*2
460 IF X<=10 AND VPEEK(0,M)<>32 THEN X=X+1:M=M+2:GOTO 460
470 IF X=11 THEN FL=FL+1:FULL(FL)=Y
480 NEXT Y
490 IF FL<>0 THEN GOSUB 510
495 RETURN

499 REM
500 REM ==== COLLAPSE AND SCORE ====
501 REM
510 FOR N=1 TO FL
520 VPOKE 1,$F9C0,255:VPOKE 1,$F9C1,7:VPOKE 1,$F9C3,%10100000
530 FOR Y=FULL(N) TO HL-1 STEP -1
540 FOR X=2 TO 21
550 IF Y=FULL(N) THEN VPOKE 1,$F9C2,192+69-(X*3)
560 VPOKE 0,(5+Y)*256+24+X,VPEEK(0,(5+Y-1)*256+24+X)
570 NEXT X
580 VPOKE 1,$F9C2,0
590 NEXT Y
600 NEXT N
610 IF FL=1 THEN SC=SC+40
611 IF FL=2 THEN SC=SC+100
612 IF FL=3 THEN SC=SC+300
613 IF FL=4 THEN SC=SC+1200
620 HL=HL+FL:LI=LI+FL
630 GOSUB 2400:REM UPDATE SCORE
640 GOSUB 2600:REM UPDATE LINES
650 IF INT(LI/10)<>INT((LI-FL)/10) THEN LV=LV+1:IF LV>25 THEN LV=25
660 GOSUB 2500:REM UPDATE LEVEL
670 RETURN

699 REM
700 REM ==== COLLISSION CONTROL ====
701 REM
710 M=(5+DY)*256+(10+DX)*2
720 I=IX(S*4+R)
